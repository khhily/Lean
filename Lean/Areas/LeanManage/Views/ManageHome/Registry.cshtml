@model CreateModel
@{
    ViewBag.Title = "Registry";
    Layout = "~/Views/Shared/_SubPageLayout.cshtml";
}


@using (Html.BeginForm())
{
    <div class="container">
        @Html.TextBlockFor(m => m.CreateUser, "col-xs-12", "150px")
        @Html.TextBlockFor(m => m.CreateModelCode, "col-xs-12", "150px")
    </div>
    <div class="panel-footer center">
        <div class="btn-group btn-group-sm">
            <input type="submit" class="btn btn-primary" value="Submit" name="Submit" id="btnSubmit" />
            <input type="button" class="btn btn-default" value="Close" name="close" id="btnClose" />
        </div>
    </div>
}

@section scripts {
    <script>
        $(function () {
            $("#btnClose").click(function () {
                WGX.ColorBox.Close();
            });
        })
    </script>
}